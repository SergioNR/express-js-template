{
    "name": "express-js-template",
    "version": "0.0.1",
    "engines": {
        "node": ">=22.0.0"
    },
    "description": "Template for Expressjs projects",
    "main": "app.mjs",
    "scripts": {
        "start": "NODE_ENV=production node ./server/app.mjs",
        "deploy:local": "node --watch --env-file=staging.env ./server/app.mjs",
        "deploy:dev": "node ./server/app.mjs",
        "test:ui": "npx playwright test --ui",
        "test:noui": "npx playwright test",
        "lint:fix": "eslint --fix ./server/*.mjs"
    },
    "author": "SNR",
    "dependencies": {
        "@getbrevo/brevo": "^2.2.0",
        "@logtail/pino": "^0.5.0",
        "axios": "^1.7.7",
        "bcryptjs": "^2.4.3",
        "connect-mongodb-session": "^5.0.0",
        "ejs": "^3.1.10",
        "express": "^5.0.1",
        "express-async-handler": "^1.2.0",
        "express-session": "^1.18.0",
        "express-validator": "^7.2.1",
        "helmet": "^8.0.0",
        "mongodb": "^6.8.0",
        "passport": "^0.7.0",
        "passport-local": "^1.0.0",
        "pino": "^9.6.0",
        "pino-mongodb": "^4.3.0",
        "pino-pretty": "^13.0.0",
        "posthog-node": "^4.1.1"
    },
    "devDependencies": {
        "@playwright/test": "^1.49.1",
        "eslint": "^8.57.1",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-plugin-import": "^2.31.0",
        "playwright": "^1.49.1"
    },
    "directories": {
        "test": "tests"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SergioNR/express-js-template.git"
    },
    "keywords": [],
    "license": "ISC",
    "type": "commonjs",
    "bugs": {
        "url": "https://github.com/SergioNR/express-js-template/issues"
    },
    "homepage": "https://github.com/SergioNR/express-js-template#readme"
}
